{
    "name": "graciousstudios/interconnect-magento2",
    "description": "Webhook module for Magento2 that channels data to the Gracious Interconnect webservice which in turn formats and proxies the data to connected consumer services",
    "type": "magento2-module",
    "license": "proprietary",
    "version": "1.4.0",
    "authors": [
        {
            "name": "gracious",
            "email": "info@gracious.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "magento/product-community-edition": ">=2.0 <3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.2 <8.0.0",
        "mockery/mockery": ">=1.1 <2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Gracious\\Interconnect\\": ""
        },
        "files": [
            "registration.php"
        ]
    },
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    }
}